<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="420.0" prefWidth="545.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" 
  fx:controller = "controller.JoinController">
   <children>
      <StackPane prefHeight="420.0" prefWidth="545.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="506.0">
               <children>
                  <Label alignment="CENTER" prefHeight="123.0" prefWidth="546.0" text="가입">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="133.0" prefWidth="546.0">
                     <children>
                        <Label text="닉네임 : ">
                           <font>
                              <Font size="26.0" />
                           </font>
                        </Label>
                        <TextField fx:id="userNameText" prefHeight="33.0" prefWidth="367.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="165.0" prefWidth="546.0">
                     <children>
                        <Button fx:id ="joinButton" mnemonicParsing="false" prefHeight="78.0" prefWidth="189.0" text="가입 및 연결" onAction="#join">
                           <font>
                              <Font size="22.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
